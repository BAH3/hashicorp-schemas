{
    "$schema": "/home/jboero/code/hashicorp-schemas/JSON/packer/1.5/manifest.json",
    "builders": [
        {
            "type": "amazon-ebs",
            "ami_name": "my-ami",
            "ami_users": [
                "user1", "user2"
            ],
            "region": "eu-central-1",
            "communicator": "ssh"
        },{
            "type": "docker",
            "author": "jboero@hc",
            "discard": false,
            "image": "my-image",
            "commit": false,
            "message": "Required field",
            "export_path": "/path/to/export"
        },{
            "type": "qemu",
            "accelerator": "kvm",
            "cpus": 8,
            "memory": 4096,
            "boot_wait": "10s",
            "disk_discard": "ignore",
            "disk_interface": "virtio",
            "net_device": "e1000"
        },{
            "type": "googlecompute"
            
        }
    ]
}